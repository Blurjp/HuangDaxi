//<![CDATA[
function checkform(form)
{
	
	
	if (form.email.value == "") 
	{
	  alert("Please enter email address");
	  form.email.focus();
	  return false;
	}
	
	if (form.password.length < 6 || form.password.length >25 ) 
	{
	  alert("Please enter password longer than 6 and shorter than 25");
	  form.password.focus();
	  return false;
	}
	
	return true;
}
</script>
